{
  "name": "rootlocal/yii2-image2",
  "description": "extension",
  "type": "yii2-extension",
  "keywords": [
    "yii2",
    "extension",
    "image"
  ],
  "homepage": "http://rootlocal.ru",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Alexander Zakharov",
      "email": "sys@eml.ru",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.0",
    "yiisoft/yii2": "~2.0.14"
  },
  "require-dev": {
    "yiisoft/yii2-codeception": "~2.0",
    "codeclimate/php-test-reporter": "^0.4.4",
    "codeception/base": "^2.4.0",
    "phpunit/phpunit": "~6.5.14",
    "codeception/verify": "~1.0.0",
    "symfony/browser-kit": ">=2.7 <=4.2.4",
    "emagister/selenium-server": "^2.52"
  },
  "autoload": {
    "psr-4": {
      "image\\": "src/"
    }
  },
  "extra": {
    "bootstrap": "image\\Bootstrap"
  },
  "asset-installer-paths": {
    "npm-asset-library": "vendor/npm",
    "bower-asset-library": "vendor/bower"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
