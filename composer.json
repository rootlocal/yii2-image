{
  "name": "rootlocal/yii2-image2",
  "description": "extension",
  "type": "yii2-extension",
  "keywords": [
    "yii2",
    "extension",
    "image"
  ],
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Alexander Zakharov",
      "email": "sys@eml.ru"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "yiisoft/yii2": "~2.0.0"
  },
  "require-dev": {
    "yiisoft/yii2-codeception": "~2.0",
    "codeclimate/php-test-reporter": "^0.4.4"
  },
  "autoload": {
    "psr-4": {
      "image\\": "src/"
    }
  },
  "extra": {
    "bootstrap": "image\\Bootstrap"
  },
  "asset-installer-paths": {
    "npm-asset-library": "vendor/npm",
    "bower-asset-library": "vendor/bower"
  }
}
